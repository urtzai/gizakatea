/***************************************/
/************* BEHEALDEA ***************/
/***************************************/

#rt-copyright {
    background-color: #F2F2F2;
    border-top: 1px solid #D7D7D7;
}

#rt-copyright .rt-block {
    padding-bottom: 5px;
    padding-top: 5px;
}

#rt-copyright .copytext {
    line-height: 33px;
}

.rt-block {
    display: block;
    margin: 10px;
    padding: 15px;
    position: relative;
}

.rt-totop {
    background-color: #33353B;
    background-image: url("http://gureeskudago.net/templates/rt_fracture/images/light/nav-arrows.png");
	background-position: 13px -275px;
    background-repeat: no-repeat;
    display: block;
    height: 40px;
    position: absolute;
    right: 0;
    top: -11px;
    transition: background-color 0.1s ease-in 0s;
    width: 40px;
}

.rt-social-buttons {
    display: inline-block;
}

.rt-social-buttons .social-button {
    background-image: url("http://gureeskudago.net/templates/rt_fracture/images/light/social-icons.png");
}
.rt-social-buttons .rt-facebook-btn {
    background-position: 0 0;
}
.rt-social-buttons .rt-twitter-btn {
    background-position: -40px 0;
}

.rt-social-buttons .rt-rss-btn {
    background-position: -80px 0;
}

.rt-social-buttons .rt-facebook-btn:hover {
    background-position: 0 -35px;
}
.rt-social-buttons .rt-twitter-btn:hover {
    background-position: -40px -35px;
}

.rt-social-buttons .rt-rss-btn:hover {
    background-position: -80px -35px;
}


.rt-social-buttons .social-button {
    background-repeat: no-repeat;
    display: block;
    float: left;
    height: 33px;
    margin-left: 8px;
    transition: width 0.2s ease-in 0s, background-color 0.2s ease-in 0s;
    width: 33px;
}


.edit_pk{
	width:47%;
	margin-right:40px;
	float:left;
}

.mapa_pk{
	width:45%;
	float:left;
}

